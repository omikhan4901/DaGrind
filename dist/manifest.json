{
    "manifest_version": 3,
    "version": "1.0",
    "name": "DaGrind",
    "permissions": [
        "tabs",
        "scripting",
        "storage"
    ],
    "content_scripts": [
        {
          "matches": ["https://*/*","http://*/*" ],
          "js": ["content-script.js"]
        }
      ],
    "action": {
        "default_popup": "index.html"
    }
}